function logoSlider2() {
 

   var swiper_logo_style2 = new Swiper('.swiper-container-logo-style2', {
      slidesPerView: 7,
      spaceBetween: 10,
      slidesPerGroup:1,
      loop:true,



      pagination: {
        el: '.swiper-pagination-logo-style2',
        clickable: true,
      },
      navigation: {
        nextEl: '.swiper-button-next-logo-style2',
        prevEl: '.swiper-button-prev-logo-style2',
      },
      keyboard: {
    enabled: true,
    onlyInViewport: true,
  },
breakpoints: {

  // when window width is <= 300px
        300: {
            slidesPerView: 3,
            spaceBetweenSlides: 10
        },
        // when window width is <= 700px
        700: {
            slidesPerView: 4,
            spaceBetweenSlides: 10
        },
        // when window width is <= 999px
        999: {
            slidesPerView: 5,
            spaceBetweenSlides: 10
        },
 // when window width is <= 1200px
        1200: {
            slidesPerView: 6,
            spaceBetweenSlides: 10
        },
 // when window width is <= 1600px
        1600: {
            slidesPerView: 8,
            spaceBetweenSlides: 10
        }
    }

    });

}

function waitForElement_logo2(){
  if(typeof swiper_logo_style2 === 'undefined'){
    if (typeof Swiper !== 'undefined'){ 
      logoSlider2();
    }
    
    else{
      setTimeout(waitForElement_logo2, 250);
    }
  }
}

waitForElement_logo2();


