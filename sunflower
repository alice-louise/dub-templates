<script type="text/javascript">

function addLogoTopBar() {
if (document.querySelector(".form-viewer") !== null) {
var logoImageHTML = '<img src='+logoImage+'>';
var tableRef = document.querySelector('.client-view-tabs-content');
var logoDiv = document.createElement('div');
    logoDiv.innerHTML = logoImageHTML;
    logoDiv.className = 'header-logo';

tableRef.parentNode.insertBefore(logoDiv, tableRef);
}}
  addLogoTopBar();
</script>
