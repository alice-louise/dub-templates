function testimonialSlider5() {

var testimonial_swiper_style5 = new Swiper('.swiper-container-style5', {
effect: 'slide',
grabCursor: true,
loop:true,
centeredSlides: true,
slidesPerView: 1,
pagination: {
el: '.swiper-pagination-style5',
clickable: true,
},



navigation: {
nextEl: '.swiper-button-next-style5',
prevEl: '.swiper-button-prev-style5',
},
keyboard: {
enabled: true,
onlyInViewport: true,
}
});


}

function waitForElementTestimonial5(){
if(typeof testimonial_swiper_style5 === 'undefined'){
if (typeof Swiper !== 'undefined'){
testimonialSlider5();


}

else{
setTimeout(waitForElementTestimonial5, 250);
}
}
}

waitForElementTestimonial5();


