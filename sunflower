<img class="hide-on-front" src="https://formsandflows.com/wp-content/uploads/2020/07/FF-Logo.png" width="0" onload="
function removePrintBG(){

	var stylesheetsList = document.styleSheets;
	for (var i = 0; i < stylesheetsList.length; i++) {

try {
			var stylesheetsListRules = stylesheetsList[i].cssRules;

		}
		catch(err) {
			break
		}
		if (stylesheetsListRules)

		for(var j=0;  j < stylesheetsListRules.length; j++){

			if(stylesheetsListRules[j].cssText.indexOf('@media print') !=-1 ){
				if(stylesheetsListRules[j].cssRules[0].selectorText == '*, ::after, ::before' ){
					stylesheetsListRules[j].deleteRule(0);
				}

			}
		}
	}
} 

removePrintBG();

">
