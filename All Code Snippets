function Hero6Slider() {
 

   var swiper_hero_style6 = new Swiper('.swiper-container-hero-style6', {
      effect: 'fade',
      grabCursor: true,
      loop:true,
      speed:800,
      centeredSlides: true,
      slidesPerView: 1,
      pagination: {
        el: '.swiper-pagination-hero-style6',
        clickable: true,
      },

      autoplay: {
    delay: 2000,
    disableOnInteraction:true,
  },
  
 
      keyboard: {
    enabled: true,
    onlyInViewport: true,
  }
    });

}

function waitForElementHero6(){
  if(typeof swiper_hero_style6 === 'undefined'){
    if (typeof Swiper !== 'undefined'){ 
      Hero6Slider();
    }
    
    else{
      setTimeout(waitForElementHero6, 250);
    }
  }
}

waitForElementHero6();


