function Hero7Slider() {
 
   var swiper_hero_style7 = new Swiper('.swiper-container-hero-style7', {
      effect: 'slide',
      loop:true,
      speed:4000,
      centeredSlides: true,
      slidesPerView: 1.5,
     navigation: {
        nextEl: '.swiper-button-next-hero-style7',
        prevEl: '.swiper-button-prev-hero-style7',
      },
      autoplay: {
    delay: 2000,
    disableOnInteraction:true,
  },
  
 
      keyboard: {
    enabled: true,
    onlyInViewport: true,
  }
    });
}
function waitForElementHero7(){
  if(typeof swiper_hero_style7 === 'undefined'){
    if (typeof Swiper !== 'undefined'){ 
      Hero7Slider();
    }
    
    else{
      setTimeout(waitForElementHero7, 250);
    }
  }
}
waitForElementHero7();
