<script type=""text/javascript"">
function addLogoTopBar() 
{

var logoImageHTML = '<img src='+logoImage+'>';

  var topBar = document.getElementById('quote-flow');

  var tableRow = topBar.getElementsByTagName('TR');

  var logo = tableRow[0].insertCell(0);

  logo.classList.add('header-logo');

  logo.innerHTML = logoImageHTML;

}

function waitTopBar(){

var topBar = document.getElementById('quote-flow');

 if (typeof logoImage !== 'undefined'){ 

 if (typeof topBar !== 'undefined'){ 


      addLogoTopBar();

    }

}

    else{

setTimeout(waitTopBar, 250);

    }

}

waitTopBar();

</script>"
