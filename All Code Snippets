function hero5Slider() {
    var swiper_hero_style5 = new Swiper('.swiper-container-hero-style5', {
        effect: 'fade',
        grabCursor: true,
        loop: true,
        speed: 800,
        centeredSlides: true,
        slidesPerView: 1,
        pagination: {
            el: '.swiper-pagination-hero-style5',
            clickable: true,
        },
        autoplay: {
            delay: 2000,
            disableOnInteraction: true,
        },
        keyboard: {
            enabled: true,
            onlyInViewport: true,
        }
    });
}


function waitForElementHero5(){
    if(typeof swiper_hero_style5 === 'undefined'){
        if (typeof Swiper !== 'undefined'){ 
            hero5Slider();
        } else {
            setTimeout(waitForElementHero5, 250);
        }
    }
}


waitForElementHero5();
