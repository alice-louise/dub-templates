function testimonialSlider4() {

var testimonial_swiper_style4 = new Swiper('.swiper-container-style4', {
effect: 'slide',
grabCursor: true,
loop:true,
centeredSlides: true,
slidesPerView: 1,
pagination: {
el: '.swiper-pagination-style4',
clickable: true,
},



navigation: {
nextEl: '.swiper-button-next-style4',
prevEl: '.swiper-button-prev-style4',
},
keyboard: {
enabled: true,
onlyInViewport: true,
}
});


}

function waitForElementTestimonial4(){
if(typeof testimonial_swiper_style4 === 'undefined'){
if (typeof Swiper !== 'undefined'){
testimonialSlider4();


}

else{
setTimeout(waitForElementTestimonial4, 250);
}
}
}

waitForElementTestimonial4();


