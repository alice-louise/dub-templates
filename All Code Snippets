<style>

/* =========================
    1 - SCROLLER - EDITABLE
   ========================= */

/* -----  1.1 - Scroller Number of logos, how many logos to fit on screen at once and scroll duration  ----- */

:root {
   --total-logos: 8;
   --show-logos:6;
   --scroll-duration: 15s;
}

@media only screen and (max-width: 600px) {
:root {

   --show-logos: 3; 
}
}


/* -----  1.2 - Scroller on Hover ----- */

#scroller:hover {
  animation-play-state:paused;
}

/* -----  1.3 - Scroller Image ----- */

.scroller-img {
  width: var(--logo-width);
  padding:15px;
}




/* =========================
    2 - SCROLLER - DON'T TOUCH
   ========================= */


/* -----  2.1 - Scroller Variables  ----- */

:root {
   --container-width2: calc(var(--container-width)*1px);
  --logo-width: calc(var(--container-width2)/var(--show-logos));
  --scroll-width: calc(var(--total-logos)*var(--logo-width));
  --scroll-width-translate: calc(var(--scroll-width) * -1 );
   --scroll-translate: calc(var(--scroll-width-translate) + var(--container-width2) );
}


/* -----  2.2 - Scroller Container ----- */

.scroller-container {
  overflow: hidden; 
  margin:25px 0px;
}

/* -----  2.3 - Scroller ----- */

#scroller {
  width:var(--scroll-width);
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.scroller-animated {
  animation-name: icon-scroller;
  animation-duration: var(--scroll-duration);
  animation-timing-function: linear;
  animation-direction: alternate;
  animation-iteration-count: infinite;
}


/* -----  2.4 - Animation ----- */

@keyframes icon-scroller {
  10% {
    transform: translate(0, 0);
  }

 90% {
      transform: translateX(var(--scroll-translate));

  }

 100% {
      transform: translateX(var(--scroll-translate));

  }
}

</style>

<img decoding=""async"" class=""hide-on-front"" src=""https://formsandflows.com/wp-content/uploads/2020/05/Forms-and-Flows.png"" onload=""
function containerWidth() {
    var width;
    var elementGroupWidth = document.getElementsByClassName('scroller-container');
    var elementIterationWidth;
    for (elementIterationWidth = 0; elementIterationWidth < elementGroupWidth.length; elementIterationWidth++) {
        var elementWidth = elementGroupWidth[elementIterationWidth];

        var positionInfo = elementWidth.getBoundingClientRect();

        width = positionInfo.width;
    }

    document.documentElement.style.setProperty('--container-width', width);
    var elementScroller = document.getElementById('scroller');
    elementScroller.classList.remove('scroller-animated');

    setTimeout(function animationAdd() {
        elementScroller.classList.add('scroller-animated');
        document.getElementById('scroller').style.animationName = 'icon-scroller';
    }, 1500)

}

containerWidth();

