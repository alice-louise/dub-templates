function imageSlider5() {
 

   var swiper_Style5 = new Swiper('.swiper-container-style5', {
      effect: 'coverflow',
      grabCursor: true,
      loop:true,
      speed:800,
      centeredSlides: true,
      slidesPerView: 2,
      coverflowEffect: {
        rotate: 50,
        stretch: 0,
        depth: 100,
        modifier: 1,
        slideShadows: false,
      },
      pagination: {
        el: '.swiper-pagination-style5',
        clickable: true,
      },
      parallax: true,
  
      navigation: {
        nextEl: '.swiper-button-next-style5',
        prevEl: '.swiper-button-prev-style5',
      },
      keyboard: {
    enabled: true,
    onlyInViewport: true,
  }
    });

}

function waitForElement5(){
  if(typeof swiper_Style5 === 'undefined'){
    if (typeof Swiper !== 'undefined'){ 
      imageSlider5();
    }
    
    else{
      setTimeout(waitForElement5, 250);
    }
  }
}

waitForElement5();


