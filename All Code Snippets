function imageSlider4() {
var galleryThumbs_style4 = new Swiper('.gallery-thumbs-style4', {
spaceBetween: 10,
slidesPerView: 5,
centeredSlides:true,
loop: true,
loopedSlides: 5, //looped slides should be the same
watchSlidesVisibility: true,
watchSlidesProgress: true,

});

var galleryTop_style4 = new Swiper('.gallery-top-style4', {
spaceBetween: 10,
loop: true,
effect:'fade',
loopedSlides: 5, //looped slides should be the same
navigation: {
nextEl: '.swiper-button-next-style4',
prevEl: '.swiper-button-prev-style4',
},
thumbs: {
swiper: galleryThumbs_style4,
},
keyboard: {
enabled: true,
onlyInViewport: true,
}
});
}

function waitForElement4(){
if(typeof galleryThumbs_style4 === 'undefined'){
if (typeof Swiper !== 'undefined'){
imageSlider4();
}

else{
setTimeout(waitForElement4, 250);
}
}
}

waitForElement4();


