function imageSlider3() {
 

   var swiper_style3 = new Swiper('.swiper-container-style3', {
      effect: 'fade',
      grabCursor: true,
      loop:true,
      speed:800,
      centeredSlides: true,
      slidesPerView: 1,
      pagination: {
        el: '.swiper-pagination-style3',
        clickable: true,
      },

      autoplay: {
    delay: 2000,
    disableOnInteraction:true,
  },
  
 
      keyboard: {
    enabled: true,
    onlyInViewport: true,
  }
    });

}

function waitForElement3(){
  if(typeof swiper_style3 === 'undefined'){
    if (typeof Swiper !== 'undefined'){ 
      imageSlider3();
    }
    
    else{
      setTimeout(waitForElement3, 250);
    }
  }
}

waitForElement3();


