function imageSlider6() {
 

   var swiper_style6 = new Swiper('.swiper-container-style6', {
      slidesPerView: 'auto',
      spaceBetween: 10,
      loop:true,
      autoplay: autoplay_slider,
autoplay: {
   delay: autoplay_speed,
 },
      pagination: {
        el: '.swiper-pagination-style6',
        clickable: true,
      },
      navigation: {
        nextEl: '.swiper-button-next-style6',
        prevEl: '.swiper-button-prev-style6',
      },
      keyboard: {
    enabled: true,
    onlyInViewport: true,
  }
    });

}

function waitForElement6(){
  if(typeof swiper_style6 === 'undefined'){
    if (typeof Swiper !== 'undefined'){ 
      imageSlider6();
    }
    
    else{
      setTimeout(waitForElement6, 250);
    }
  }
}

waitForElement6();


