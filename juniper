<img class="hide-on-front" src="https://formsandflows.com/wp-content/uploads/2020/05/Forms-and-Flows.png" onload = "

function uncheckRadio() {

 jQuery('input[name=faq-tab]').click(function(){
  var jQueryradio = jQuery(this);

        // if this was previously checked
        if (jQueryradio.data('waschecked') == true)
        {
          jQueryradio.prop('checked', false);
          jQueryradio.data('waschecked', false);
        }
        else
          jQueryradio.data('waschecked', true);
        
        // remove was checked from other radios
        jQueryradio.siblings('input[name=faq-tab]').data('waschecked', false);
      });

}
uncheckRadio();

